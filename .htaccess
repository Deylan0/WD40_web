### PROTECT FILES ###
<FilesMatch "\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
Order Allow,Deny
Deny from all
</FilesMatch>

RewriteEngine on
### RewriteRule <Index-alias> index.html ###
RewriteRule mainPage index.html
RewriteRule signin signin.php
RewriteRule signinForm form.php
RewriteRule signed signed.php
